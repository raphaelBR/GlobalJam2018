/// @description  browser_name(os_browser)
/// @function  browser_name
/// @param os_browser

var browser = argument[0]

if browser == browser_not_a_browser
    {
    return "NOT A BROWSER"
    }
if browser == browser_unknown
    {
    return "UNKNOWN"
    }
if browser == browser_ie
    {
    return "INTERNET EXPLORER"
    }
if browser == browser_firefox
    {
    return "MOZILLA FIREFOX"
    }
if browser == browser_chrome
    {
    return "GOOGLE CHROME"
    }
if browser == browser_safari
    {
    return "SAFARI"
    }
if browser == browser_opera
    {
    return "OPERA"
    }
if browser == browser_safari_mobile
    {
    return "SAFARI MOBILE"
    }
if browser == browser_windows_store
    {
    return "WINDOWS 8 APP"
    }
